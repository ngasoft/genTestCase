-- the following two files are generated by python script
include "Vehicle_Compromise.csp"
include "Vehicle_Compromise_test_cases.csp"

channel attack_succeed
Attack_End = attack_succeed -> STOP

Test_Purpose = Attacker ; Attack_End

SystemEvents = diff(Events, {attack_succeed})

UnsecureSystem = RUN(SystemEvents)

assert UnsecureSystem [] Test_Cases [T= UnsecureSystem [| SystemEvents |] Test_Purpose
